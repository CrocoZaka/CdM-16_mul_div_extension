{
    "name": "cdm-devkit(?)",
    "displayName": "cdm devkit(?)",
    "description": "Assembler for cdm processors",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "author": {
        "name": "cdm developers(?)"
    },
    "icon": "icon.png",
    "publisher": "cdm developers(?)",
    "homepage": "https://github.com/cdm-processors/cdm-devkit",
    "bugs": "https://github.com/cdm-processors/cdm-devkit/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/cdm-processors/cdm-devkit.git"
    },
    "license": "BSD",
    "keywords": [
        "assembly",
        "macro",
        "assembler",
        "cdm8",
        "asm",
        "cdm16",
        "cdm",
        "cdm8e"
    ],
    "contributes": {
        "languages": [
            {
                "id": "cdm8-assembly",
                "aliases": [
                    "cdm8 assembly",
                    "cdm8-assembly"
                ],
                "extensions": [
                    ".asm .h"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "cdm16-assembly",
                "aliases": [
                    "cdm16 assembly",
                    "cdm16-assembly"
                ],
                "extensions": [
                    ".asm .h"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cdm8-assembly",
                "scopeName": "source.asm.cdm8",
                "path": "./syntaxes/cdm8-assembly.tmLanguage.json"
            },
            {
                "language": "cdm16-assembly",
                "scopeName": "source.asm.cdm16",
                "path": "./syntaxes/cdm16-assembly.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "cdm8-assembly",
                "path": "./snippets/cdm8-assembly-snippets.json"
            },
            {
                "language": "cdm16-assembly",
                "path": "./snippets/cdm16-assembly-snippets.json"
            }
        ]
    },
    "__metadata": {
        "publisherDisplayName": "cdm developers(?)"
    }
}
